{
    "name": "dyutin/laravel-file-manager",
    "description": "Laravel file manager",
    "type": "library",
    "keywords": ["laravel", "file manager", "vue file manager","file browser"],
    "license": "MIT",
    "authors": [
        {
            "name": "Timur Dyutin",
            "email": "rustemovv96@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-zip": "*",
        "symfony/process": "^4.4|5.0",
        "illuminate/support": "^6.0|^7.0",
        "illuminate/console": "^6.0|^7.0",
        "illuminate/config": "^6.0|^7.0",
        "illuminate/filesystem": "^6.0|^7.0",
        "illuminate/routing": "^6.0|^7.0",
        "illuminate/view": "^6.0|^7.0"
    },
    "require-dev": {
       "orchestra/testbench": "^4.0|^5.0",
       "phpunit/phpunit": "^8.0|^8.5"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "dyutin\\FileManager\\": "src/",
            "dyutin\\FileManager\\Tests\\": "tests/"
        }
    },
    "scripts": {
       "test": "vendor/bin/phpunit",
       "test-coverage": "vendor/bin/phpunit --coverage-text"
    },
    "extra": {
        "laravel": {
            "providers": [
                "dyutin\\FileManager\\FileManagerServiceProvider"
            ]
        }
    }
}
